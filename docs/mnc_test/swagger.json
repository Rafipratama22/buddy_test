                "summary": "Retrieves all users point",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "/company/user/register": {
            "get": {
                "description": "Retrieves the list of users who has to register in the app",
                "consumes": [
                    "*/*"
