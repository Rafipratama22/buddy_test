            "get": {
                "description": "Retrieves the list of users who has to login in the app",
                "produces": [
                    "application/json"
                ],
                "summary": "Retrieves the list of users who has to login in the app",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "additionalProperties": true
                            }
                        }
                    }
                }
            }
        },
        "/company/user/point": {
            "get": {
                "description": "Retrieves all users point",
                "produces": [
                    "application/json"
